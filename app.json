{
  "expo": {
    "name": "Magic The Gathering app",
    "slug": "MagicApp",
    "privacy": "public",
    "sdkVersion": "34.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "android": {
      "package": "com.oddOtocyon.magicTheGatheringApp"
    },
    "ios": {
      "bundleIdentifier": "com.oddOtocyon.magicTheGatheringApp",
      "supportsTablet": true
    },
    "version": "0.5.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ]
  }
}